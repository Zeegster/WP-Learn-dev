{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ulitka-kit/media-text-absolute",
	"version": "0.1.0",
	"title": "Текст на изображении",
	"category": "ulitka-custom",
	"icon": "slides",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false,
		"className": true,
		"spacing": { "padding": true }
	},
	"attributes": {
		"zIndex": {
			"type": "integer"
		},
		"innerBlockPosition": {
			"type": "string",
			"enum": [
				"center-center",
				"center-right",
				"center-left",
				"top-center",
				"top-right",
				"top-left",
				"bottom-center",
				"bottom-right",
				"bottom-left"
			],
			"default": "center-center"
		},
		"imageURL": {
			"type": "string"
		},
		"imageID": {
			"type": "number"
		},
		"imageWidth": {
			"type": "number",
			"default": " "
		},
		"imageHeight": {
			"type": "number",
			"default": " "
		}
	},
	"textdomain": "example-dynamic",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
